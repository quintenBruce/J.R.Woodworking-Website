<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="J.R.Woodworking is a custom furniture manufacturer serving the Lubbock, TX region. We specialize in moder/farmhouse wood furniture but also have experience in metal fabrication so no job is out of our skillset.">
    <title>@ViewData["Title"] - J.R. Woodworking</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="stylesheet" href="~/css/styless.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/bootswatchTheme.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WebApplication1.styles.css" asp-append-version="true" />
</head>
<body>
    <script>
        console.log("sadfsdf")
      var chatbox = document.getElementById('fb-customer-chat');
      chatbox.setAttribute("page_id", "104366531142816");
      chatbox.setAttribute("attribution", "biz_inbox");
    </script>

    <!-- Your SDK code -->
    <script>
        console.log("sadffsdfgfjhjgj")
      window.fbAsyncInit = function() {
        FB.init({
          xfbml            : true,
          version          : 'v14.0'
        });
      };

      (function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = 'https://connect.facebook.net/en_US/sdk/xfbml.customerchat.js';
        fjs.parentNode.insertBefore(js, fjs);
      }(document, 'script', 'facebook-jssdk'));
    </script>
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
            <div class="container-fluid">
                <a style="padding-left: 3%;"class="navbar-brand" asp-controller="Home" asp-action="Index">J.R. Woodworking</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarColor01">
                <ul class="navbar-nav me-auto">
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Contact" asp-action="Index">Contact Us</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Products" asp-action="ProductsIndex">Products</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Policies" asp-action="PoliciesIndex">Policies</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Shop By...</a>
                        <div class="dropdown-menu bg-primary">
                            <a class="dropdown-item" asp-controller="Products" asp-action="LivingRoom">Living Room</a>
                            <a class="dropdown-item" asp-controller="Products" asp-action="DiningRoom">Dining Room</a>
                            <a class="dropdown-item" asp-controller="Products" asp-action="BedRoom">Bedroom</a>
                            <a class="dropdown-item" asp-controller="Products" asp-action="BathRoom">Bathroom</a>
                            <a class="dropdown-item" asp-controller="Products" asp-action="Outdoors">Outdoors</a>
                        </div>
                    </li>
                </ul>
                @*<partial name="_LoginPartialCustom" />*@
                <ul class="navbar-nav">
                    <div class="navbar-nav me-auto">
                        <a href="https://www.facebook.com/JamesReedWoodworking">
                            <img class="fb-logo" src="https://res.cloudinary.com/dmremrlru/image/upload/v1651707364/5296499_fb_facebook_facebook_logo_icon_alga4o.png" />
                        </a>
                        @*<li class="nav-item">
                            <a class="nav-link" asp-controller="About" asp-route-returnurl="@ViewData["ReturnUrl"]" asp-action="Index">About</a>
                        </li>*@
                    </div>
                </ul>
            </div>
            </div>
        </nav>
    </header>
    <div class="">
        <main role="main" class="">
            @RenderBody()
        </main>
    </div>

    <footer class="bg-primary border-top footer text-muted">
        <div class="container">
            &copy; 2022 - J.R Woodworking
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <!-- Messenger Chat Plugin Code -->
    <div id="fb-root"></div>

    <!-- Your Chat Plugin code -->
    <div id="fb-customer-chat" class="fb-customerchat">
    </div>

    
</body>
</html>
